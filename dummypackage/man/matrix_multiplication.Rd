## The function is currently defined as
function (a, b) 
{
    if (is.matrix(a) & is.matrix(b)) {
        if (ncol(a) != nrow(b)) {
            stop("Number of cols in the first matrix must be equal to the number of rows in the second matrix.")
        }
        return(a \%*\% b)
    }
    else {
        stop("Both arguments must be matrices.")
    }
  }
}
